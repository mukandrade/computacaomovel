<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eduardo.escaradedecubitoapp" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:screenOrientation="portrait"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".TelaPrincipal_activity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TelaConfiguracoes_activity"
            android:label="@string/title_activity_tela_configuracoes_activity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".TelaPaciente_activity"
            android:label="@string/title_activity_tela_paciente_activity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".TelaRecomendacoes_activity"
            android:label="@string/title_activity_tela_recomendacoes_activity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".TelaSobre_activity"
            android:label="@string/title_activity_tela_sobre_activity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".Coleta_activity"
            android:label="@string/title_activity_coleta_activity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".CadastroPaciente_activity"
            android:label="@string/title_activity_cadastro_paciente_activity" >
        </activity>
        <activity
            android:name=".TelaHistorico_activity"
            android:label="@string/title_activity_tela_historico_activity" >
        </activity>
    </application>

</manifest>
